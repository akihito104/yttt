    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/tttFragment"
    >
    <fragment
        android:id="@+id/tttFragment"
        android:name="com.freshdigitable.yttt.TimetableTabFragment"
        tools:layout="@layout/fragment_timetable_tab"
